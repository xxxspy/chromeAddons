{
	"manifest_version":2,
	"name":"TOEFL",
	"version":"1.0",
	"background":{
		"scripts":["config.js", "utils.js","background.js"],
		"persistent":false
	},
	"browser_action":{
		"default_icon":"btn.ico",
		"default_popup":"popup.html"
	},
	"permissions":[
		"notifications", "activeTab", "tabs", "*://*.etest.net.cn/*", "http://127.0.0.1:3303/*"
	]
}